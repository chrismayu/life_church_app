<%= simple_form_for @life_pulse_stock_picture, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.hidden_field :key %>
 
  <%= f.input :title %>
 
 
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                life_pulse_stock_pictures_path, :class => 'btn' %>
  </div>
<% end %>
