<%- model_class = LifePulseStockPicture -%>
<div class="page-header">
  <h1><%= @life_pulse_stock_picture.title.html_safe %></h1>
</div>

<div>
	<p>
<%= display_life_pulse_stock_picture(@life_pulse_stock_picture) %>
</p>
<br><br><br><br>
 

</div>


<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              life_pulse_stock_pictures_path, :class => 'btn btn-flat'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_life_pulse_stock_picture_path(@life_pulse_stock_picture), :class => 'btn btn-flat' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              life_pulse_stock_picture_path(@life_pulse_stock_picture),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger btn-flat' %>
</div>
