<%- model_class = EventForm -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:event_setup_form_id) %>:</strong></dt>
  <dd><%= @event_form.event_setup_form_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:first_name) %>:</strong></dt>
  <dd><%= @event_form.first_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:last_name) %>:</strong></dt>
  <dd><%= @event_form.last_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:receive_emails) %>:</strong></dt>
  <dd><%= @event_form.receive_emails %></dd>
  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
  <dd><%= @event_form.email %></dd>
  <dt><strong><%= model_class.human_attribute_name(:phone_number) %>:</strong></dt>
  <dd><%= @event_form.phone_number %></dd>
  <dt><strong><%= model_class.human_attribute_name(:notes) %>:</strong></dt>
  <dd><%= @event_form.notes %></dd>
  <dt><strong><%= model_class.human_attribute_name(:c_f_1_s) %>:</strong></dt>
  <dd><%= @event_form.c_f_1_s %></dd>
  <dt><strong><%= model_class.human_attribute_name(:c_f_2_s) %>:</strong></dt>
  <dd><%= @event_form.c_f_2_s %></dd>
  <dt><strong><%= model_class.human_attribute_name(:c_f_3_s) %>:</strong></dt>
  <dd><%= @event_form.c_f_3_s %></dd>
  <dt><strong><%= model_class.human_attribute_name(:c_f_4_s) %>:</strong></dt>
  <dd><%= @event_form.c_f_4_s %></dd>
  <dt><strong><%= model_class.human_attribute_name(:c_f_5_b) %>:</strong></dt>
  <dd><%= @event_form.c_f_5_b %></dd>
  <dt><strong><%= model_class.human_attribute_name(:c_f_6_b) %>:</strong></dt>
  <dd><%= @event_form.c_f_6_b %></dd>
  <dt><strong><%= model_class.human_attribute_name(:c_f_7_b) %>:</strong></dt>
  <dd><%= @event_form.c_f_7_b %></dd>
  <dt><strong><%= model_class.human_attribute_name(:c_f_8_b) %>:</strong></dt>
  <dd><%= @event_form.c_f_8_b %></dd>
  <dt><strong><%= model_class.human_attribute_name(:c_f_9_t) %>:</strong></dt>
  <dd><%= @event_form.c_f_9_t %></dd>
  <dt><strong><%= model_class.human_attribute_name(:c_f_10_t) %>:</strong></dt>
  <dd><%= @event_form.c_f_10_t %></dd>
  <dt><strong><%= model_class.human_attribute_name(:c_f_11_t) %>:</strong></dt>
  <dd><%= @event_form.c_f_11_t %></dd>
  <dt><strong><%= model_class.human_attribute_name(:c_f_12_t) %>:</strong></dt>
  <dd><%= @event_form.c_f_12_t %></dd>
  <dt><strong><%= model_class.human_attribute_name(:age) %>:</strong></dt>
  <dd><%= @event_form.age %></dd>
  <dt><strong><%= model_class.human_attribute_name(:bday) %>:</strong></dt>
  <dd><%= @event_form.bday %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sex) %>:</strong></dt>
  <dd><%= @event_form.sex %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              event_forms_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_event_form_path(@event_form), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              event_form_path(@event_form),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
