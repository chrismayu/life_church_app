<h1>Listing schedules</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>End month</th>
    <th>Start month</th>
    <th>Schedule image</th>
    <th>Image processed</th>
    <th>Slug</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @schedules.each do |schedule| %>
  <tr>
    <td><%= schedule.name %></td>
    <td><%= schedule.description %></td>
    <td><%= schedule.end_month %></td>
    <td><%= schedule.start_month %></td>
    <td><%= schedule.schedule_image %></td>
    <td><%= schedule.image_processed %></td>
    <td><%= schedule.slug %></td>
    <td><%= schedule.url %></td>
    <td><%= link_to 'Show', schedule %></td>
    <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
    <td><%= link_to 'Destroy', schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Schedule', new_schedule_path %>
