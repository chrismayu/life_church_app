<%- model_class = MinistryPictureChild -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:ministrychild_image) %>:</strong></dt>
  <dd><%= @ministry_picture_child.ministrychild_image %></dd>
  <dt><strong><%= model_class.human_attribute_name(:image_processed) %>:</strong></dt>
  <dd><%= @ministry_picture_child.image_processed %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ministrychild_id) %>:</strong></dt>
  <dd><%= @ministry_picture_child.ministrychild_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              ministry_picture_children_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_ministry_picture_child_path(@ministry_picture_child), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              ministry_picture_child_path(@ministry_picture_child),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
